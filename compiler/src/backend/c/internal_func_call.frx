import "c.frx";
import "assert.frx";
import "sizeof.frx";
import "../../frontend/ast/internal_func_call.frx";

export void transpile_internal_func_call(Transpiler* transpiler, InternalFuncCall* internal_func_call)
{
    frx::assert(transpiler != nullptr);

    frx::assert(internal_func_call != nullptr);

    switch(internal_func_call->type)
    {
        case InternalFuncCallType::ASSERT:
        {
            transpile_assert(transpiler, internal_func_call->assert_node);

            break;
        }

        case InternalFuncCallType::SIZEOF:
        {
            transpile_sizeof(transpiler, internal_func_call->sizeof_node);

            break;
        }

        default:
        {
            frx::assert(false);

            break;
        }
    }
}
