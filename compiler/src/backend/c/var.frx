export void transpile_var(Transpiler* transpiler, ASTVar* var)
{
    assert(transpiler != nullptr);

    assert(var != nullptr);

    if(var->resolved_ns != nullptr)
    {
        transpile_namespace_resolution(transpiler, var->resolved_ns);
    }
    else
    {
        transpile_namespace_resolution(transpiler, var->ns);
    }

    fprintf(transpiler->file, "%s", var->name);
}
