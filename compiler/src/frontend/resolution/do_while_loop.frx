import "../ast/do_while_loop.frx";
import "../core/parser.frx";
import "expr.frx";
import "scope.frx";

export void do_while_loop_resolve_symbols(Parser* parser, DoWhileLoop* do_while_loop)
{
    frx::assert(parser != nullptr);

    frx::assert(do_while_loop != nullptr);

    scope_resolve_symbols(parser, do_while_loop->scope);
    expr_resolve_symbols(parser, do_while_loop->condition);
}
