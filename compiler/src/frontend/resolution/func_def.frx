import "../ast/func_def.frx";
import "../core/parser.frx";
import "func_params.frx";
import "scope.frx";
import "type_specifier.frx";

export void func_def_resolve_symbols(Parser* parser, FuncDef* func_def)
{
    frx::assert(parser != nullptr);

    frx::assert(func_def != nullptr);

    type_specifier_resolve_symbols(parser, func_def->return_type);

    func_params_resolve_symbols(parser, func_def->params);
    scope_resolve_symbols(parser, func_def->body);
}
