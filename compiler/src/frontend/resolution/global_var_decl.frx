import "../ast/global_var_decl.frx";
import "../core/parser.frx";
import "type_specifier.frx";

export void global_var_decl_resolve_symbols(Parser* parser, GlobalVarDecl* global_var_decl)
{
    frx::assert(parser != nullptr);

    frx::assert(global_var_decl != nullptr);

    type_specifier_resolve_symbols(parser, global_var_decl->symbol.type);
}
