import "../ast/global_var_def.frx";
import "../core/parser.frx";
import "type_specifier.frx";
import "expr.frx";

export void global_var_def_resolve_symbols(Parser* parser, GlobalVarDef* global_var_def)
{
    frx::assert(parser != nullptr);

    frx::assert(global_var_def != nullptr);

    type_specifier_resolve_symbols(parser, global_var_def->symbol.type);
    expr_resolve_symbols(parser, global_var_def->value);
}
