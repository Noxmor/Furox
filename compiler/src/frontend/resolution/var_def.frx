import "../ast/var_def.frx";
import "../core/parser.frx";
import "type_specifier.frx";
import "expr.frx";

export void var_def_resolve_symbols(Parser* parser, VarDef* var_def)
{
    frx::assert(parser != nullptr);

    frx::assert(var_def != nullptr);

    type_specifier_resolve_symbols(parser, var_def->symbol.type);
    expr_resolve_symbols(parser, var_def->value);
}
