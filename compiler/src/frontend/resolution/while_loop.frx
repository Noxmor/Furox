import "../ast/while_loop.frx";
import "../core/parser.frx";
import "expr.frx";
import "scope.frx";

export void while_loop_resolve_symbols(Parser* parser, WhileLoop* while_loop)
{
    frx::assert(parser != nullptr);

    frx::assert(while_loop != nullptr);

    expr_resolve_symbols(parser, while_loop->condition);
    scope_resolve_symbols(parser, while_loop->scope);
}
