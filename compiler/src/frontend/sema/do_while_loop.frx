import "expr.frx";
import "scope.frx";
import "../core/parser.frx";
import "../ast/do_while_loop.frx";

export void do_while_loop_sema(Parser* parser, DoWhileLoop* do_while_loop)
{
    frx::assert(parser != nullptr);

    frx::assert(do_while_loop != nullptr);

    scope_sema(parser, do_while_loop->scope);
    expr_sema(parser, do_while_loop->condition);
}
