import "func_decl.frx";
import "../core/parser.frx";
import "../ast/extern_block.frx";
import "../ast/func_decl.frx";
import "../../core/list.frx";

namespace AST::ExternBlock
{
    export void sema(Parser* parser, ExternBlock* extern_block)
    {
        frx::assert(parser != nullptr);

        frx::assert(extern_block != nullptr);

        mut usize i;
        for(i = 0; i < List::size(&extern_block->func_decls); i = i + 1)
        {
            FuncDecl* func_decl = List::get(&extern_block->func_decls, i);
            AST::FuncDecl::sema(parser, func_decl);
        }
    }
}
