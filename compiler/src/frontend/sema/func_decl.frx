import "func_params.frx";
import "type_specifier.frx";
import "../core/errors.frx";
import "../core/parser.frx";
import "../symbols/func.frx";
import "../ast/func_decl.frx";
import "../../core/list.frx";

export void func_decl_sema(mut Parser* parser, mut FuncDecl* func_decl)
{
    frx::assert(parser != nullptr);

    frx::assert(func_decl != nullptr);

    type_specifier_sema(parser, func_decl->return_type);

    func_params_sema(parser, func_decl->params);
}
