import "../core/parser.frx";
import "../ast/initializer_list.frx";
import "expr.frx";

export void initializer_list_sema(Parser* parser, InitializerList* list)
{
    frx::assert(parser != nullptr);

    frx::assert(list != nullptr);

    mut usize i;
    for(i = 0; i < list_size(&list->values); i = i + 1)
    {
        Expr* value = list_get(&list->values, i);
        expr_sema(parser, value);
    }
}
