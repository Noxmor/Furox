import "../core/parser.frx";
import "../ast/initializer_list.frx";
import "expr.frx";

namespace AST::InitializerList
{
    export void sema(Parser* parser, InitializerList* list)
    {
        frx::assert(parser != nullptr);

        frx::assert(list != nullptr);

        mut usize i;
        for(i = 0; i < List::size(&list->values); i = i + 1)
        {
            Expr* value = List::get(&list->values, i);
            AST::Expr::sema(parser, value);
        }
    }
}
