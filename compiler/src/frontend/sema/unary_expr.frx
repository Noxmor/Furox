import "expr.frx";
import "../core/parser.frx";
import "../ast/unary_expr.frx";

namespace AST::UnaryExpr
{
    export void sema(Parser* parser, UnaryExpr* expr)
    {
        frx::assert(parser != nullptr);

        frx::assert(expr != nullptr);

        AST::Expr::sema(parser, expr->operand);
    }
}
