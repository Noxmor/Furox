import "expr.frx";
import "scope.frx";
import "../core/parser.frx";
import "../ast/while_loop.frx";

namespace AST::WhileLoop
{
    export void sema(Parser* parser, WhileLoop* while_loop)
    {
        frx::assert(parser != nullptr);

        frx::assert(while_loop != nullptr);

        AST::Expr::sema(parser, while_loop->condition);
        AST::Scope::sema(parser, while_loop->scope);
    }
}
