import "expr.frx";
import "scope.frx";
import "../core/parser.frx";
import "../ast/while_loop.frx";

export void while_loop_sema(Parser* parser, WhileLoop* while_loop)
{
    frx::assert(parser != nullptr);

    frx::assert(while_loop != nullptr);

    expr_sema(parser, while_loop->condition);
    scope_sema(parser, while_loop->scope);
}
