import "expr.frx";
import "scope.frx";
import "../core/parser.frx";
import "../ast/do_while_loop.frx";

export void analyze_do_while_loop(Parser* parser, AST::DoWhileLoop* do_while_loop)
{
    frx::assert(parser != nullptr);

    frx::assert(do_while_loop != nullptr);

    analyze_scope(parser, do_while_loop->scope);
    analyze_expr(parser, do_while_loop->condition);
}
