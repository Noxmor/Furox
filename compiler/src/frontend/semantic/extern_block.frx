import "func_decl.frx";
import "../core/parser.frx";
import "../ast/extern_block.frx";
import "../ast/func_decl.frx";
import "../../core/list.frx";

export void analyze_extern_block(Parser* parser, AST::ExternBlock* extern_block)
{
    frx::assert(parser != nullptr);

    frx::assert(extern_block != nullptr);

    mut usize i;
    for(i = 0; i < list_size(&extern_block->func_decls); i = i + 1)
    {
        AST::FuncDecl* func_decl = list_get(&extern_block->func_decls, i);
        analyze_func_decl(parser, func_decl);
    }
}
