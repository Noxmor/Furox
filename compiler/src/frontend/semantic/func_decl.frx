export void analyze_func_decl(Parser* parser, ASTFuncDecl* func_decl)
{
    assert(parser != nullptr);

    assert(func_decl != nullptr);

    FuncSymbol* symbol = &func_decl->symbol;

    b8 is_redefinition = false;

    usize i;
    for(i = 0; i < list_size(&parser->imported_parsers) && !is_redefinition;
        i = i + 1)
    {
        Parser* imported_parser = list_get(&parser->imported_parsers, i);
        is_redefinition = func_table_find(&imported_parser->func_table,
            symbol->ns, symbol->name) != nullptr;
    }

    if(is_redefinition)
    {
        //FIXME: Print the correct location, this is only a placeholder
        SourceLocation loc;
        error_func_decl_redefinition(parser_source_file(parser), loc,
            symbol->name);

        parser->failed = true;
    }

    analyze_func_params(parser, func_decl->params);
}
