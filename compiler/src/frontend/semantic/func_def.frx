export void analyze_func_def(Parser* parser, ASTFuncDef* func_def)
{
    assert(parser != nullptr);

    assert(func_def != nullptr);

    if(func_def->symbol == nullptr)
    {
        //FIXME: Print the correct location, this is only a placeholder
        SourceLocation loc;
        error_func_redefinition(parser_source_file(parser), loc,
            func_def->name);

        parser->failed = true;
    }

    analyze_scope(parser, func_def->body);
}
