import "../core/parser.frx";
import "../ast/initializer_list.frx";
import "expr.frx";

export void analyze_initializer_list(Parser* parser, AST::InitializerList* list)
{
    frx::assert(parser != nullptr);

    frx::assert(list != nullptr);

    mut usize i;
    for(i = 0; i < list_size(&list->values); i = i + 1)
    {
        AST::Expr* value = list_get(&list->values, i);
        analyze_expr(parser, value);
    }
}
