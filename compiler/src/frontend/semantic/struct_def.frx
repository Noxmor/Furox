export void analyze_struct_def(Parser* parser, ASTStructDef* struct_def)
{
    assert(parser != nullptr);

    assert(struct_def != nullptr);

    StructSymbol* symbol = &struct_def->symbol;

    b8 is_redefinition = false;

    usize i;
    for(i = 0; i < list_size(&parser->imported_parsers) && !is_redefinition;
        i = i + 1)
    {
        Parser* imported_parser = list_get(&parser->imported_parsers, i);
        is_redefinition = struct_table_find(&imported_parser->struct_table,
            symbol->ns, symbol->name) != nullptr;
    }

    if(is_redefinition)
    {
        //FIXME: Print the correct location, this is only a placeholder
        SourceLocation loc;
        error_struct_redefinition(parser_source_file(parser), loc,
            symbol->name);

        parser->failed = true;
    }
}
