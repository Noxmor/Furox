export import "../ast/char_literal.frx";
import "../core/parser.frx";

export CharLiteral* char_literal_parse(Parser* parser)
{
    frx::assert(parser != nullptr);

    char* literal = parser->token->identifier;

    parser_eat(parser, TokenType::CHAR_LITERAL);

    return char_literal_new(&parser->arena, literal);
}
