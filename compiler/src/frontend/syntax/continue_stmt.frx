export import "../ast/continue_stmt.frx";
import "../core/parser.frx";

namespace AST::ContinueStmt
{
    export ContinueStmt* parse(Parser* parser)
    {
        Parser::eat(parser, TokenType::KW_CONTINUE);

        Parser::eat(parser, TokenType::SEMICOLON);

        return new(&parser->arena);
    }
}
