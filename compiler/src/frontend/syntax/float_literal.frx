export import "../ast/float_literal.frx";
import "../core/parser.frx";

export FloatLiteral* float_literal_parse(Parser* parser)
{
    frx::assert(parser != nullptr);

    FloatLiteral* literal = float_literal_new(&parser->arena, parser->token->float_literal);

    parser_eat(parser, TokenType::FLOAT_LITERAL);

    return literal;
}
