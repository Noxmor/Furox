export import "../ast/float_literal.frx";
import "../core/parser.frx";

namespace AST::FloatLiteral
{
    export FloatLiteral* parse(Parser* parser)
    {
        frx::assert(parser != nullptr);

        FloatLiteral* literal = new(&parser->arena, parser->token->float_literal);

        parser_eat(parser, TokenType::FLOAT_LITERAL);

        return literal;
    }
}
