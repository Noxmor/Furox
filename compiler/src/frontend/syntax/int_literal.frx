export import "../ast/int_literal.frx";
import "../core/parser.frx";

namespace AST::IntLiteral
{
    export IntLiteral* parse(Parser* parser)
    {
        frx::assert(parser != nullptr);

        IntLiteral* literal = new(&parser->arena, parser->token->int_literal);

        parser_eat(parser, TokenType::INT_LITERAL);

        return literal;
    }
}
