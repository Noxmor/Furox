export import "../ast/int_literal.frx";
import "../core/parser.frx";

export IntLiteral* int_literal_parse(Parser* parser)
{
    frx::assert(parser != nullptr);

    IntLiteral* literal = int_literal_new(&parser->arena, parser->token->int_literal);

    parser_eat(parser, TokenType::INT_LITERAL);

    return literal;
}
