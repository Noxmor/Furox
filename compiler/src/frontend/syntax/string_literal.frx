export import "../ast/string_literal.frx";
import "../core/parser.frx";

namespace AST::StringLiteral
{
    export StringLiteral* parse(Parser* parser)
    {
        frx::assert(parser != nullptr);

        char* literal = parser->token->identifier;

        parser_eat(parser, TokenType::STRING_LITERAL);

        return new(&parser->arena, literal);
    }
}
